{
    "name": "screenify",
    "displayName": "Screenify",
    "description": "screenify your code snippet capturer",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/screenify/screenify-vscode.git"
    },
    "publisher": "AdamMomen",
    "galleryBanner": {
        "color": "#fbfbfb",
        "theme": "light"
    },
    "icon": "icon.png",
    "activationEvents": [
        "onCommand:screenify.activate",
        "onWebviewPanel:screenify",
        "onView:getting-started"
    ],
    "main": "./src/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [{
                "id": "screenfiy",
                "title": "Screenfiy",
                "icon": "resources/icon.png"
            }]
        },
        "views": {
            "screenfiy": [{
                "id": "getting-started",
                "name": "Getting Started"
            }, {
                "id": "Help",
                "name": "help"
            }]
        },
        "menu"
        "view/item/context": {

        },
        "commands": [{
            "command": "screenify.activate",
            "title": "Screenify ðŸ“¸",
            "icon": {
                "light": "resources/icon.png",
                "dark": "resources/icon.png"
            }
        }],
        "keybindings": [{
            "command": "screenify.activate",
            "key": "ctrl+shift+s",
            "mac": "cmd+shift+s"
        }],
        "configuration": {
            "title": "Screenify",
            "properties": {
                "screenify.shadow": {
                    "type": "string",
                    "description": "Shadow of the snippet node. Use any value for CSS `box-shadow`",
                    "default": "rgba(0, 0, 0, 0.55) 0px 20px 68px"
                },
                "screenify.transparentBackground": {
                    "type": "boolean",
                    "description": "Transparent background for containers",
                    "default": false
                },
                "screenify.backgroundColor": {
                    "type": "string",
                    "description": "Background color of snippet container. Use any value for CSS `background-color`",
                    "format": "color-hex",
                    "default": "#f2f2f2"
                },
                "screenify.target": {
                    "type": "string",
                    "description": "Shoot with or without container",
                    "default": "container",
                    "enum": [
                        "container",
                        "snippet"
                    ],
                    "enumDescriptions": [
                        "Shoot with the container.",
                        "Shoot with the snippet alone. If you want transparent padding, use `container` with `\"screenify.transparentBackground\": true`"
                    ]
                },
                "screenify.serverUrl": {
                    "type": "string",
                    "description": "`",
                    "default": "screenify-njy7ok457q-ez.a.run.app"
                }
            }
        }
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.1",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.42.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.0.1",
        "typescript": "^3.7.5",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "@google-cloud/storage": "^4.7.0",
        "@simonwep/pickr": "^1.5.1",
        "bluebird": "^3.7.2",
        "cloudinary": "^1.21.0",
        "fs": "0.0.1-security",
        "node-fetch": "^2.6.0",
        "node-powershell": "^4.0.0",
        "os": "^0.1.1",
        "path": "^0.12.7",
        "randomstring": "^1.1.5"
    }
}